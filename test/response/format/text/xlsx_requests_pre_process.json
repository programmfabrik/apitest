{
    "name": "Get existing XLSX file, preprocess: check number of bytes",
    "request": {
        "server_url": {{ datastore "req_base_url" | marshal }},
        "endpoint": "test.xlsx",
        "method": "GET"
    },
    "response": {
        "statuscode": 200,
        "format": {
            "type": "text",
            "pre_process": {
                "cmd": {
                    "name": "wc",
                    "args": [
                        "-c"
                    ]
                }
            }
        },
        "body": {
            "text_trimmed": "101024",
            "float64": 101024,
            "int64": 101024
        }
    }
}