{
    "name": "Get existing CSV file, check content in text format",
    "request": {
        "server_url": {{ datastore "req_base_url" | marshal }},
        "endpoint": "dummy.csv",
        "method": "GET"
    },
    "response": {
        "statuscode": 200,
        "format": {
            "type": "text",
            "pre_process": {
                "cmd": {
                    "name": "wc",
                    "args": [
                        "-c"
                    ]
                }
            }
        },
        "body": {
             "text": "      42\n"
        }
    }
}