{
    "name": "bounce HTML file, use response format \"html\"",
    "request": {
        "server_url": "{{ datastore "req_base_url" }}",
        "endpoint": "bounce",
        "method": "POST",
        "body": {
            "file": "@sample.html"
        },
        "body_type": "multipart"
    },
    "response": {
        "format": {
            "type": "html"
        },
        "body": {{ file "result_html.json" }}
    }
}